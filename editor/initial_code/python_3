def preco_final(preco, desconto):
    # escreva aqui seu código
    return None


print("Example:")
print(preco_final(100, 0.1))

# As funções assert são utilizadas para verificar se o resultado da função chamada é o esperado

assert preco_final(100, 0.25) == 75.0 

assert preco_final(50, 0.1) == 45.0 

assert preco_final(75.5, 0.5) == 37.75

# testa a função sem desconto    
assert preco_final(55.5, 0) == 75.5

# testa a função com 100% de descontro
assert preco_final(89.9, 1) == 0

    
print("Seu código passou pelos testes, clique em Check para avançar!")
